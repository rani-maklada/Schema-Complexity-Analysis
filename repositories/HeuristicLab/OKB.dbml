}

Table dbo.Characteristic {
  Id Int64 [primary key]
  Name String
  Type CharacteristicType
}

Table dbo.CharacteristicValue {
  Id Int64 [primary key]
  ProblemId Int64
  CharacteristicId Int64
  DataTypeId Int64
  BoolValue Boolean
  IntValue Int32
  LongValue Int64
  FloatValue Single
  DoubleValue Double
  StringValue String
}

Table dbo.SingleObjectiveSolution {
  Id Int64 [primary key]
