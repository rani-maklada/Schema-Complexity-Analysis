Table dbo.AlgorithmClass {
  Id Int64 [primary key]
  Name String
  Description String
}

Table dbo.ProblemClass {
  Id Int64 [primary key]
  Name String
  Description String
}

Table dbo.AlgorithmUser {
  AlgorithmId Int64 [primary key]
  UserGroupId Guid [primary key]
}

Table dbo.ProblemUser {
  ProblemId Int64 [primary key]
  UserGroupId Guid [primary key]
}

Table dbo.Platform {
  Id Int64 [primary key]
  Name String
  Description String
}

Table dbo.DataType {
  Id Int64 [primary key]
  Name String
  TypeName String
}

Table dbo.ValueName {
  Id Int64 [primary key]
  Name String
  Category ValueNameCategory
  Type ValueNameType
}

Table dbo.Algorithm {
  Id Int64 [primary key]
  AlgorithmClassId Int64
  PlatformId Int64
  DataTypeId Int64
  BinaryDataId Int64
  Name String
  Description String
}

Table dbo.Problem {
  Id Int64 [primary key]
  ProblemClassId Int64
  PlatformId Int64
  DataTypeId Int64
  BinaryDataId Int64
  Name String
  Description String
}

Table dbo.BinaryData {
  Id Int64 [primary key]
