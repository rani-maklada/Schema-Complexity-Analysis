  End DateTime
  HiFiveLimit Int32
}

Table dbo.Friendships {
  Befriender String [primary key]
  Befriendee String [primary key]
}

Table dbo.Points {
  PointID Int32 [primary key]
  Username String
  Issued DateTime
  Amount Int32
