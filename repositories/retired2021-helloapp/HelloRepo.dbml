}

Table dbo.Redemptions {
  RedemptionID Int32 [primary key]
  Username String
  TokenID Int32
  Created DateTime
}

Table dbo.TagAggregates {
  TagAggregateID Int32
  Tag String
  UserTypeID String
  Count Int32
}

Table dbo.Messages {
  Username String [primary key]
  Text String
  Offensive Boolean
}

Table dbo.Tags {
  Name String [primary key]
  Username String [primary key]
  Created DateTime
}

Table dbo.TideMarks {
  TideMarkID Int32 [primary key]
  Name String
